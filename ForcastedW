import tkinter as tk
import MethodZ
class Forecasted_Weather():
    def __init__(self,N_FWwindow):
        self.N_FWwindow=N_FWwindow
        self.backbutton=tk.Button(N_FWwindow, text='back', command=self.N_FWwindow.destroy)
        self.backbutton.pack()
        self.N_FWwindow.geometry("250x250")

